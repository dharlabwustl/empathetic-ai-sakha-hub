
# Sample .env.staging file

# API Configuration
VITE_APP_ENVIRONMENT=staging
VITE_API_URL_DEVELOPMENT=http://localhost:5000/api/v1
VITE_API_URL_STAGING=https://staging-api.sakha.ai/api/v1
VITE_API_URL_PRODUCTION=https://api.sakha.ai/api/v1

# Feature Flags
VITE_ENABLE_TUTOR_CHAT=true
VITE_ENABLE_FEEL_GOOD=true
VITE_ENABLE_MOOD_TRACKING=true
VITE_ENABLE_SURROUNDING_INFLUENCES=true

# Analytics
VITE_ANALYTICS_ENABLED=true

# Limits
VITE_MAX_FILE_UPLOAD_SIZE=10485760 # 10MB
